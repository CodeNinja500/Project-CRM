{
    "inputs": {
      "input-default": "<input type=\"$type$\" formControlName=\"$formControlName$\" placeholder=\"$label$\">",
      "input-submit":"<button class=\"btn btn-primary w-100\" (click)=\"$submitCallback$\">Send</button>",
      "input-text-area":"<textarea class=\"form-control mb-3\" formControlName=\"$formControlName$\" placeholder=\"$label$\" rows=\"8\"></textarea>"
    },
    "layouts": {
      "section": "<section></section>",
      "container-fluid-no-padding": "<main class=\"main\" id=\"top\"><div class=\"container-fluid px-0\"></div></main>",
      "container-fluid": "<div class=\"container-fluid\"></div>",
      "card-centered":"<div class=\"row flex-center position-relative min-vh-100 g-0 py-5\"><div class=\"col-11 col-sm-10 col-xl-8\"><div class=\"card border border-300 auth-card\"><div class=\"card-body pe-md-0\"></div></div></div></div>",
      "col-bg-col-content":"<div class=\"row align-items-center mx-3 gx-0 gy-5\"><div class=\"col-auto bg-100 dark__bg-1100 rounded-3 position-relative overflow-hidden auth-title-box\"><div class=\"bg-holder card-col-bg\" ></div><div class=\"position-relative px-4 pt-7 pb-7 pb-sm-5 pb-lg-7 text-center text-md-start px-md-7\"><div class=\"false\"><a href=\"./index.html\"><div class=\"d-flex align-items-center justify-content-center justify-content-md-start\"><img src=\"./assets/img/icons/logo.png\" alt=\"phoenix\" width=\"48\" /></div></a><h3 class=\"mt-3 text-black\">Login to your account</h3><a class=\"fw-bold fs--1\" href=\"./pages/authentication/card/sign-up.html\">Or create one now<span class=\"fas fa-angle-right ms-1\" data-fa-transform=\"down-1 shrink-2\"></span></a></div></div><div class=\"position-relative z-index--1 mb-6 d-none d-sm-block text-center mt-md-15\"><img class=\"auth-title-box-img d-dark-none\" src=\"./assets/img/spot-illustrations/auth.png\" alt=\"\" /><img class=\"auth-title-box-img d-light-none\" src=\"./assets/img/spot-illustrations/auth-dark.png\" alt=\"\" /></div></div><div class=\"col mx-auto\"></div></div>",
      "login-form":"<div class=\"auth-form-box\"><form [formGroup]=\"loginForm\"><div class=\"mb-3 text-start\"><label class=\"form-label\" for=\"email\">Email address</label><div class=\"form-icon-container\"><input class=\"form-control form-icon-input\" type=\"email\" placeholder=\"name@example.com\" formControlName='email' /><span class=\"fas fa-user text-900 fs--1 form-icon\"></span></div></div><div class=\"mb-3 text-start\"><label class=\"form-label\" for=\"password\">Password</label><div class=\"form-icon-container\"><input class=\"form-control form-icon-input\" type=\"password\" placeholder=\"Password\" formControlName='password' /><span class=\"fas fa-key text-900 fs--1 form-icon\"></span></div></div><div class=\"row flex-between-center mb-7\"><div class=\"col-auto\"><div class=\"form-check mb-0\"><input class=\"form-check-input\" id=\"basic-checkbox\" type=\"checkbox\" formControlName='rememberMe' /><label class=\"form-check-label mb-0\" for=\"basic-checkbox\">Remember me</label></div></div><div class=\"col-auto\"><a class=\"fs--1 fw-semi-bold\" href=\"./pages/authentication/card/forgot-password.html\">Forgot Password?</a></div></div><button class=\"btn btn-primary w-100 mb-3\">Sign In</button><div class=\"text-center\"><a class=\"fs--1 fw-bold\" href=\"./pages/authentication/card/sign-up.html\">Create an account</a></div></form></div>",
      "centered-image-with-text":"<div class=\"text-center\"><a class=\"d-flex flex-center text-decoration-none mb-4\" href=\"/index.html\"><div class=\"d-flex align-items-center fw-bolder fs-5 d-inline-block\"><img src=\"../../../assets/img/icons/logo.png\" alt=\"phoenix\" width=\"58\" /></div></a><h4 class=\"text-1000\">Your account is not active</h4><p class=\"text-700 mb-5\">Check your email's inbox</p></div>",
      "avatar-with-text":"<div class=\"text-center mb-5\"><div class=\"avatar avatar-4xl mb-4\"><img class=\"rounded-circle\" src=\"./assets/img/team/30.webp\" alt=\"\" /></div><h2 class=\"text-1000\"> <span class=\"fw-normal\">Hello </span>John Smith</h2><p class=\"text-700\">Write down something about yourself</p></div>",
      "navbar":"<nav class=\"navbar navbar-top navbar-expand\" id=\"navbarDefault\"><div class=\"collapse navbar-collapse justify-content-between\"><div class=\"navbar-logo\"><a class=\"navbar-brand me-1 me-sm-3\" href=\"./index.html\"><div class=\"d-flex align-items-center\"><div class=\"d-flex align-items-center\"><img src=\"./assets/img/icons/logo.png\" alt=\"phoenix\" width=\"27\" /><p class=\"logo-text ms-2 d-none d-sm-block\">phoenix</p></div></div></a></div><ul class=\"navbar-nav navbar-nav-icons flex-row\"><li class=\"nav-item dropdown\"><a class=\"nav-link lh-1 pe-0\" id=\"navbarDropdownUser\" role=\"button\" data-bs-toggle=\"dropdown\" data-bs-auto-close=\"outside\" aria-haspopup=\"true\" aria-expanded=\"false\" (click)=\"toggleUserMenu()\"><div class=\"avatar avatar-l \"><img class=\"rounded-circle \" src=\"../../assets/img/team/40x40/57.webp\" alt=\"\" /></div></a><div class=\"dropdown-menu dropdown-menu-end navbar-dropdown-caret py-0 dropdown-profile shadow border border-300\" [ngClass]=\"{show: isUserMenuVisible$|async}\" aria-labelledby=\"navbarDropdownUser\" data-bs-popper=\"static\"><div class=\"card position-relative border-0\"><div class=\"card-body p-0\"><div class=\"text-center pt-4 pb-3\"><div class=\"avatar avatar-xl \"><img class=\"rounded-circle \" src=\"assets/img/team/72x72/57.webp\" alt=\"\"></div><h6 class=\"mt-2 text-black\">Jerry Seinfield</h6></div></div><div class=\"card-footer p-0 border-top\"><div class=\"px-3 mt-3\"><a class=\"btn btn-phoenix-secondary d-flex flex-center w-100\" (click)=\"signUserOut()\"><span data-feather=\"log-out\"></span>Sign out</a></div><div class=\"my-2 text-center fw-bold fs--2 text-600\"><a class=\"text-600 me-1\">Privacy policy</a>&bull;<a class=\"text-600 mx-1\">Terms</a>&bull;<a class=\"text-600 ms-1\">Cookies</a></div></div></div></div></li></ul></div></nav>",
      "content-with-footer":"<div class=\"content\"><div class=\"pb-9\"><!--CONTENT--></div><footer class=\"footer position-absolute\"><div class=\"row g-0 justify-content-between align-items-center h-100\"><div class=\"col-12 col-sm-auto text-center\"><p class=\"mb-0 mt-2 mt-sm-0 text-900\">Thank you for creating with Phoenix<span class=\"d-none d-sm-inline-block\"></span><span class=\"d-none d-sm-inline-block mx-1\">|</span><br class=\"d-sm-none\" />2023 &copy;</p></div><div class=\"col-12 col-sm-auto text-center\"><p class=\"mb-0 text-600\">v1.9.0</p></div></div></footer></div>",
      "large-header":"<h2 class=\"mb-4\">2 Companies</h2>"
    },
    "forms": {
      "form": {
        "input": "$inputs.input-default$",
        "formSubmit": "<input type=\"submit\" value=\"$content$\">",
        "form": "<form [formGroup]=\"$formVariable$\" (ngSubmit)=\"$submitCallback$\">$content$</form>"
      },
      "form-text-boxes": {
        "input": "$inputs.input-text-area$",
        "formSubmit": "$inputs.input-submit$",
        "form": "<div class=\"auth-form-box\"><form [formGroup]=\"$formVariable$\">$content$</form></div>"
      }
    },
    "lists": {
      "ul": {
        "list": "<ul>$content$</ul>",
        "item": "<li *ngFor=\"let $itemVariable$ of $listVariable$\">$content$</li>"
      },
      "table": {
        "list": "<table>$content$</table>",
        "item": "<tr *ngFor=\"let $itemVariable$ of $listVariable$\"><td>$content$</td></tr>"
      },
      "table-bootstrap": {
        "list": "<div class=\"px-4 mx-lg-n6 px-lg-6\"><div class=\"table-responsive scrollbar mx-n1 px-1 border-top\"><table class=\"table fs--1 mb-0 leads-table\" *ngIf=\"$listVariable$ as list\"><thead><tr><th class=\"sort white-space-nowrap align-middle text-uppercase\" scope=\"col\">Col name</th></tr></thead><tbody class=\"list\" id=\"deal-tables-body\">$content$</tbody></table></div></div>",
        "item": "<tr class=\"hover-actions-trigger btn-reveal-trigger position-static\" *ngFor=\"let $itemVariable$ of list\"><td class=\"align-middle white-space-nowrap fw-semi-bold text-1000 ps-4 border-end\"><div class=\"d-flex align-items-center\"><div>$content$</div></div></td></tr>"
      }
    },
    "details": {
      "content": {
        "container": "<ng-container *ngIf=\"$detailVariable$ | async as $itemVariable$\">$content$</ng-container>",
        "content": "$content$"
      },
      "text": {
        "container": "<ng-container *ngIf=\"$detailVariable$ | async as $itemVariable$\">$content$</ng-container>",
        "content": "{{ $itemVariable$ }}"
      }
    }
  }
  